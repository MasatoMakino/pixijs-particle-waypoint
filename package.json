{
  "name": "@masatomakino/pixijs-particle-waypoint",
  "version": "0.3.0",
  "description": "Particle waypoint module for pixi.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./esm/index.js",
  "files": [
    "lib",
    "esm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://MasatoMakino@github.com/MasatoMakino/pixijs-particle-waypoint.git"
  },
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@tweenjs/tween.js": "18.6.4",
    "@types/node": "^17.0.41",
    "dat.gui": "^0.7.9",
    "@masatomakino/gulptask-demo-page": "0.4.4",
    "@masatomakino/gulptask-dev-server": "0.4.1",
    "husky": "^8.0.1",
    "pixi.js": "^6.4.2",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@masatomakino/particle-waypoint": "0.5.3"
  },
  "peerDependencies": {
    "@tweenjs/tween.js": ">=18.5.0 <19.0.0",
    "pixi.js": ">=5.2.0 <7.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "start:dev": "npm run server & npm run watch:tsc & npm run watch:demo",
    "build": "npm run buildTS && npm run demo",
    "demo": "npx @masatomakino/gulptask-demo-page",
    "buildTS": "npx tsc --project tsconfig.json & npx tsc --project tsconfig.esm.json",
    "typedocs": "npx typedoc --out ./docs/api src/index.ts",
    "server": "npx @masatomakino/gulptask-dev-server -b ./docs/demo",
    "watch:demo": "npm run demo -- -W",
    "watch:tsc": "npx tsc -W --incremental --project tsconfig.json & npx tsc -W --incremental --project tsconfig.esm.json"
  },
  "bugs": {
    "url": "https://github.com/MasatoMakino/pixijs-particle-waypoint/issues"
  },
  "homepage": "https://github.com/MasatoMakino/pixijs-particle-waypoint#readme",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  }
}
